{
  "manifest_version": 2,

  "name": "Bookmarks to JSON",
  "version": "1.0",
  "description": "Exports your bookmarks in JSON format",
  "icons": { 
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png", 
    "256": "icons/256x256.png",
    "512": "icons/512x512.png"
  },
  "browser_action": {
    "default_icon": "icons/256x256.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "bookmarks"
  ]
}